{
    "appName": "My HTTP Server - Development",
    "config": {
        "listenerPort": 3000,
        "enableAccessLogs": true,
        "static": {
            "enabled": true,
            "path": "./public"
        },
        "log": {
            "rootLevel": "ERROR",
            "exceptions": [
                {"package": "test.*","logLevel": "INFO"}
            ]
        },
        "enableViewTemplates": true
    }
}